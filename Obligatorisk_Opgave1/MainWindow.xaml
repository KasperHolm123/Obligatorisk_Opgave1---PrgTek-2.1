<Window x:Class="Obligatorisk_Opgave1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Obligatorisk_Opgave1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DataGrid Width="200" Height="200" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="100,0,0,0" AutoGenerateColumns="False">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Navn" Binding="{Binding StartVampList}"/>
                <DataGridTextColumn Header="Prioritet" Binding="{Binding EndVampList}"/>
            </DataGrid.Columns>
        </DataGrid>
        <DataGrid Width="200" Height="200" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="500,0,0,0" AutoGenerateColumns="False">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Navn"/>
                <DataGridTextColumn Header="Tidspunkt"/>
            </DataGrid.Columns>
        </DataGrid>
        <Button Content="Tag opkald" HorizontalAlignment="Left" Margin="295,56,0,0" VerticalAlignment="Top" Command=""/>
        <Button Content="Afslut opkald" HorizontalAlignment="Left" Margin="451,56,0,0" VerticalAlignment="Top" Command=""/>

    </Grid>
</Window>
